@model IEnumerable<Student>

@{ 
   ViewBag.Title = "Student List";
}
    

<table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Course</th>
            </tr>
        </thead>
                
        <tbody>
            @foreach(var student in Model)
            {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Course</td>
            </tr>
            }
        </tbody>
     </table>
       


